      </Label>
      <Label layoutX="14.0" layoutY="140.0" text="Fecha fin:">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <TextField fx:id="workPlanKeyTextField" layoutX="238.0" layoutY="50.0" prefHeight="24.0" prefWidth="324.0" />
