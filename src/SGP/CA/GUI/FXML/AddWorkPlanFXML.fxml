<AnchorPane prefHeight="200.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="SGP.CA.GUI.AddWorkPlanController">
   <children>
      <Rectangle arcHeight="5.0" arcWidth="5.0" fill="#b2b2b2" height="41.0" stroke="BLACK" strokeType="INSIDE" width="600.0" />
      <Label layoutX="14.0" layoutY="10.0" text="Nuevo plan de trabajo">
         <font>
            <Font name="System Bold" size="18.0" />
