<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.shape.Rectangle?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="SGP.CA.GUI.ModifyWorkPlanController">
   <children>
      <Label layoutX="30.0" layoutY="40.0" text="ADMINISTRACIÓN DE PLAN DE TRABAJO">
         <font>
            <Font name="System Bold" size="39.0" />
         </font>
      </Label>
      <Rectangle arcHeight="5.0" arcWidth="5.0" fill="#9e9e9e" height="492.0" layoutX="30.0" layoutY="108.0" stroke="BLACK" strokeType="INSIDE" width="826.0" />
      <Rectangle arcHeight="5.0" arcWidth="5.0" fill="WHITE" height="157.0" layoutX="214.0" layoutY="125.0" stroke="BLACK" strokeType="INSIDE" width="578.0" />
      <Rectangle arcHeight="5.0" arcWidth="5.0" fill="WHITE" height="252.0" layoutX="214.0" layoutY="348.0" stroke="BLACK" strokeType="INSIDE" width="578.0" />
      <Label layoutX="226.0" layoutY="141.0" text="Clave del plan de trabajo:">
         <font>
            <Font size="19.0" />
         </font>
      </Label>
      <Label layoutX="226.0" layoutY="180.0" text="Fecha de inicio:">
         <font>
            <Font size="19.0" />
         </font>
      </Label>
      <Label layoutX="226.0" layoutY="224.0" text="Fecha de fin:">
         <font>
            <Font size="19.0" />
         </font>
      </Label>
      <Button fx:id="addObjectiveButton" layoutX="214.0" layoutY="293.0" mnemonicParsing="false" onAction="#addObjectiveEvent" text="Agregar objetivo">
         <font>
            <Font size="19.0" />
         </font>
      </Button>
      <ComboBox fx:id="objectivesComboBox" layoutX="226.0" layoutY="374.0" prefHeight="24.0" prefWidth="295.0" promptText="[Selecciona un objetivo]" />
      <Button fx:id="modifyComboBox" layoutX="521.0" layoutY="370.0" mnemonicParsing="false" onAction="#modifyObjectiveEvent" prefHeight="33.0" prefWidth="109.0" text="Modificar">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Button fx:id="saveButton" layoutX="52.0" layoutY="474.0" mnemonicParsing="false" onAction="#saveButtonEvent" text="GUARDAR">
         <font>
            <Font size="19.0" />
         </font>
      </Button>
      <Button fx:id="exitButton" layoutX="52.0" layoutY="535.0" mnemonicParsing="false" onAction="#exitButtonEvent" prefHeight="36.0" prefWidth="111.0" text="SALIR">
         <font>
            <Font size="19.0" />
         </font>
      </Button>
      <TextField fx:id="workPlanKeyTextField" layoutX="450.0" layoutY="141.0" prefHeight="24.0" prefWidth="240.0" />
      <TextField fx:id="startDateTextField" layoutX="373.0" layoutY="180.0" />
      <TextField fx:id="endDateTextField" layoutX="373.0" layoutY="224.0" />
      <Button fx:id="addToPlanButton" layoutX="642.0" layoutY="369.0" mnemonicParsing="false" onAction="#addToPlanEvent" prefHeight="36.0" prefWidth="141.0" text="Añadir al plan">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <ComboBox fx:id="objectivesAddedComboBox" layoutX="226.0" layoutY="436.0" prefHeight="24.0" prefWidth="295.0" promptText="[Objetivos añadidos al plan]" />
   </children>
</AnchorPane>
